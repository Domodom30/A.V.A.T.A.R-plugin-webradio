                "textColor": "rgba(255, 255, 255, 1)"
            },
            "window": {
                "width": 375,
                "height": 138,
                "opacity": 1
            },
            "devTools": false,
            "notification": 3000,
            "active": true,
            "selection": {
                "radio": "fun radio",
                "favoris": ["rire et chanson", "nrj"]
            },
            "search": {
                "nbradio": 75
            }
        }
