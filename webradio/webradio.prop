{
    "modules": {
        "webradio": {
            "version": "2.3.0",
            "name": "webradio",
            "devices": {},
            "clients": {
                "Salon": "Salon"
            },
            "rules": {
                "play": [
                    "(~put~|~play~) the? radio",
                    "listen to? the? radio"
                ],
                "stop": [
                    "(~stop~|~cut~) the? radio"
                ]
            },
            "widget": {
                "display": true,
                "autoUpdate": {
                    "delay": 30,
                    "start": false
